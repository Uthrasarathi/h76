#include<stdio.h>
#include<conio.h>
void main()
{
float m1,m2,s1,s2,s3,s4,avg,per;
printf("Enter 6 subject marks:\n");
scanf("%f %f %f %f %f %f",&m1,&m2,&s1,&s2,&s3,&s4);
per=(m1+m2+s1+s2+s3+s4)*100/600;
printf("students gets %0.2f per");
if(per>=60)
printf("1st class\n");
else if(per>=50 && per<=59)
printf("2nd class");
else if(per>=40 && per<=49)
printf("3ed class");
else if(per<40)
printf("fail");
getch();
}
